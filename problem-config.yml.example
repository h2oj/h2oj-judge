# Copy this file to your problem data directory
# and named as 'config.yml'

# Config for running a judge task

# mode(string): judge mode
# - oi: OI mode
# - acm: ACM mode 
mode: oi

# type(string): judge type
# - default: default mode
# - spj: special judge mode
# - pretest: codeforces mode
type: default

# substack_count(number): substack count
substack_count: 1

# substack(array): detail for each substack
substack:
  - case_count: 1 # case count
    # score_type(string): score type
    # - sum:
    # - max:
    # - min:
    score_type: sum
    case: # detail for each test case
      - max_space: 1024
        max_time: 1000
        score: 50
      - max_space: 1024
        max_time: 1000
        score: 50
